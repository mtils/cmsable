{
  "name": "ems/cmsable",
  "type": "library",
  "description": "A library to build cms-powered applications with laravel",
  "keywords": ["cms", "scaffolding"],
  "license": "MIT",
  "authors": [
    {
      "name": "Michael Tils",
      "email": "mtils@web-utils.de",
      "homepage": "https://web-utils.de"
    }
  ],
  "require": {
    "php": ">=8.2.0",
    "laravel/framework": "11.*",
    "ems/framework": ">=0.5.1",
    "ems/collection": "0.3.*",
    "ems/xtype": "0.3.*",
    "ems/beetree":"0.3.*",
    "ems/formobject": "0.4.*",
    "ems/versatile": "0.3.*"
  },
  "require-dev": {
    "phpunit/phpunit": "11.1.2",
    "mockery/mockery": "~1.6"
  },
  "autoload": {
    "psr-4": {
      "Cmsable\\": "src/Cmsable"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Cmsable\\Test\\": "tests/unit"
    }
  }
}
